 sinh s\u1ed1ng v\u00e0 l\u00e0m vi\u1ec7c t\u1ea1i M\u1ef9. V\u00ec v\u1eady nhi\u1ec1u kh\u00e1ch h\u00e0ng c\u1ee7a ISO Logistics mu\u1ed1n bi\u1ebft gi\u00e1 c\u01b0\u1edbc&#8230;<\/p>\n","protected":false},"author":3,"featured_media":5524,"comment_status":"closed","ping_status":"closed","sticky":false,"template":"","format":"standard","meta":{"footnotes":""},"categories":[1],"tags":[],"class_list":["post-7797","post","type-post","status-publish","format-standard","has-post-thumbnail","hentry","category-tin-chuyen-nganh"],"_links":{"self":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts\/7797"}],"collection":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/users\/3"}],"replies":[{"embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/comments?post=7797"}],"version-history":[{"count":5,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts\/7797\/revisions"}],"predecessor-version":[{"id":8132,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts\/7797\/revisions\/8132"}],"wp:featuredmedia":[{"embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/media\/5524"}],"wp:attachment":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/media?parent=7797"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/categories?post=7797"},{"taxonomy":"post_tag","embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/tags?post=7797"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}