 sinh Vi\u1ec7t Nam sang sinh s\u1ed1ng v\u00e0 h\u1ecdc t\u1eadp. B\u00ean c\u1ea1nh \u0111\u00f3 c\u1ed9ng \u0111\u1ed3ng ng\u01b0\u1eddi Vi\u1ec7t t\u1ea1i M\u1ef9&#8230;<\/p>\n","protected":false},"author":3,"featured_media":0,"comment_status":"open","ping_status":"open","sticky":false,"template":"","format":"standard","meta":{"footnotes":""},"categories":[59,1],"tags":[],"class_list":["post-1519","post","type-post","status-publish","format-standard","hentry","category-epacket","category-tin-chuyen-nganh"],"_links":{"self":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts\/1519"}],"collection":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/users\/3"}],"replies":[{"embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/comments?post=1519"}],"version-history":[{"count":3,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts\/1519\/revisions"}],"predecessor-version":[{"id":5746,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/posts\/1519\/revisions\/5746"}],"wp:attachment":[{"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/media?parent=1519"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/categories?post=1519"},{"taxonomy":"post_tag","embeddable":true,"href":"https:\/\/iso-logistics.vn\/wp-json\/wp\/v2\/tags?post=1519"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}